{% if header_to_render == "awdc" %}
{% set deptlogo, divlogo = "logo_white.svg", "jswa_logo_white.png" %}
{% else %}
{% set deptlogo, divlogo = "logo.svg", "jswa_logo.png" %}
{% endif %}
<header role="banner" class="{{ header_to_render }}">
	<div class="c-logo-holder">
		<div class="logo-generic c-govt-logo"><a href="//dtwd.wa.gov.au"><img src="/{{ directory }}/images/header/{{ deptlogo }}" alt="Department of Training and Workforce Development logo" /><span>Department of Training and Workforce Development</span></a></div>
		<div class="logo-generic c-jswa-logo"><a href="/"><img src="/{{ directory }}/images/header/{{ divlogo }}" alt="Jobs and Skills Western Australia logo" /><span>Home</span></a></div>
	</div>

	<div class="c-search-box">
		<div class="c-search-top">
			<nav>
				<ul>
					<li><a href="{{ site_map }}">Site map</a></li>
					<li><a href="{{ accessibility }}">Accessibility</a></li>
					<li><a href="{{ contact_us }}">Contact us</a></li>
				</ul>
			</nav>
		</div>
		<div class="c-search-middle">
			<div class="js-form-item form-item js-form-type-search form-item-keys js-form-item-keys form-no-label">
				<form action="/search">
					<label for="edit-keys" class="visually-hidden">Search</label>
					<input placeholder="Search this site..." title="Enter the terms you wish to search for." data-drupal-selector="edit-keys" type="search" id="edit-keys" name="keys" value="" size="15" maxlength="128" class="form-search">
					<input data-drupal-selector="edit-submit" type="submit" id="edit-submit" value="Search" class="button js-form-submit form-submit">
				</form>
			</div>
		</div>
		<div class="c-search-bottom">
			<a href="//wa.gov.au">Go to whole of WA Government search <i class="fa fa-external-link" aria-hidden="true"></i></a>
		</div>
	</div>
</header>

<div id="i-primary-menu">
	{{ page.menu }}
	<div class="c-menu-social-media">
		{% if twitter %}
		<div class="c-social-generic c-twitter"><a href="{{ twitter }}" target="_blank"><i class="fa fa-twitter" aria-hidden="true"></i><span class="c-hide-text">Twitter</span></a></div>
		{% endif %}
		{% if facebook %}
		<div class="c-social-generic c-facebook"><a href="{{ facebook }}" target="_blank"><i class="fa fa-facebook" aria-hidden="true"></i><span class="c-hide-text">Facebook</span></a></div>
		{% endif %}
		{% if linked_in %}
		<div class="c-social-generic c-lined-in"><a href="{{ linked_in }}" target="_blank"><i class="fa fa-linkedin" aria-hidden="true"></i><span class="c-hide-text">Linked In</span></a></div>
		{% endif %}
		{% if youtube %}
		<div class="c-social-generic c-youtube"><a href="{{ youtube }}" target="_blank"><i class="fa fa-youtube-play" aria-hidden="true"></i><span class="c-hide-text">YouTube</span></a></div>
		{% endif %}
	</div>
</div>